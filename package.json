{
  "private": true,
  "type": "module",
  "name": "chitchat",
  "version": "0.1.0",
  "description": "ChatGPT Chatbot Experiment",
  "keywords": [
    "chatgpt",
    "chatbot"
  ],
  "author": "Ed Preston",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.17.0"
  },
  "packageManager": "npm@9.8.0",
  "browserslist": [
    "Chrome >= 112, ChromeAndroid >= 112, Edge >= 112, Firefox >= 112, FirefoxAndroid  >= 112,  Safari >= 16.4, iOS >= 16.4, not dead",
    "maintained node versions"
  ],
  "scripts": {
    "dev": "vite",
    "dev-host": "vite --host",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview",
    "preview-host": "vite preview --host",
    "start": "vite --open",
    "lint": "eslint src/**/*.js test/**/*.js cli/**/*.js",
    "test": "vitest",
    "test-run": "vitest run",
    "test-coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^0.33.0",
    "@vitest/web-worker": "^0.33.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "jsdom": "^22.1.0",
    "vite": "^4.4.5",
    "vitest": "^0.33.0"
  }
}
